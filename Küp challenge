
    x = int(input("Bir say覺 giriniz:"))
    y = int(input("Bir say覺 giriniz:"))
    z = int(input("Bir say覺 giriniz:"))
    n = int(input("Bir say覺 giriniz:"))
    valid_permutations = []
    for i in [0, x]:
        for j in [0, y]:
            for k in [0, z]:
                arr = [i, j, k ]
                if sum(arr) != n:
                    valid_permutations.append(arr)
                    
    print(valid_permutations)
